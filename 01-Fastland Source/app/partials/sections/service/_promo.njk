{% macro render(options) %}
    <div class="promo-area promo-area--inner-service bg-electric-violet-2">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-xl-8">
                    <div class="promo-content dark-mode-texts text-center">
                        <h2 class="promo-content__title">Interested in joining our team? </h2>
                        <button class="btn btn-primary shadow--primary-5 btn--medium-5 rounded-50 text-white mt-5 mx-2">Get a quote</button>
                        <button class="btn btn-outline-white btn-2 rounded-50 text-white mt-5 mx-2">Explore our works</button>
                    </div>
                </div>
            </div>
        </div>
        <div class="shape">
            <img class="w-100" src="./image/png/inner-banner-shape.png" alt="">
        </div>
    </div>
{% endmacro %}