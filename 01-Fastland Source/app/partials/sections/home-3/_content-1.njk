{% macro render(options) %}
<div class="content-section content-section--l3-1 bg-default-3">
    <div class="container">
        <div class="row align-items-center justify-content-center">
            <div class="col-xl-5 col-lg-6 col-md-8 col-xs-10 order-2 order-lg-1">
